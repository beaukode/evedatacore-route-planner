{
  "openapi": "3.1.0",
  "info": {
    "title": "evedatacore-route-planner",
    "description": "",
    "license": {
      "name": ""
    },
    "version": "0.1.0"
  },
  "paths": {
    "/near": {
      "post": {
        "tags": [],
        "summary": "Find the nearest stars to a given star",
        "description": "Returns the nearest stars to a given star",
        "operationId": "calc_near",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/NearResult"
                }
              }
            }
          }
        }
      }
    },
    "/path": {
      "post": {
        "tags": [],
        "operationId": "calc_path",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PathResult"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "ConnType": {
        "type": "string",
        "enum": [
          "gate",
          "smartgate",
          "jump"
        ]
      },
      "NearResult": {
        "type": "object",
        "required": [
          "connections"
        ],
        "properties": {
          "connections": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/PathResultConnection"
            }
          }
        }
      },
      "PathResult": {
        "type": "object",
        "required": [
          "status",
          "path",
          "stats"
        ],
        "properties": {
          "path": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/PathResultConnection"
            }
          },
          "stats": {
            "$ref": "#/components/schemas/PathResultStats"
          },
          "status": {
            "$ref": "#/components/schemas/PathResultStatus"
          }
        }
      },
      "PathResultConnection": {
        "type": "object",
        "required": [
          "conn_type",
          "distance",
          "target",
          "id"
        ],
        "properties": {
          "conn_type": {
            "$ref": "#/components/schemas/ConnType"
          },
          "distance": {
            "type": "integer",
            "format": "int32",
            "minimum": 0
          },
          "id": {
            "type": "integer",
            "format": "int32",
            "minimum": 0
          },
          "target": {
            "type": "integer",
            "format": "int32",
            "minimum": 0
          }
        }
      },
      "PathResultStats": {
        "type": "object",
        "required": [
          "cost",
          "total_time",
          "successors_spend",
          "loop_spend",
          "visited"
        ],
        "properties": {
          "cost": {
            "type": "integer",
            "format": "int64"
          },
          "loop_spend": {
            "type": "integer",
            "minimum": 0
          },
          "successors_spend": {
            "type": "integer",
            "minimum": 0
          },
          "total_time": {
            "type": "integer",
            "minimum": 0
          },
          "visited": {
            "type": "integer",
            "format": "int64",
            "minimum": 0
          }
        }
      },
      "PathResultStatus": {
        "type": "string",
        "enum": [
          "found",
          "notfound",
          "timeout"
        ]
      }
    }
  }
}